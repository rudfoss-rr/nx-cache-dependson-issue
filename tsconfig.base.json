{
	"compilerOptions": {
		"composite": true,
		"declarationMap": true,
		"importHelpers": true,
		"isolatedModules": true,
		"lib": ["ESNext"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2024",
		"resolveJsonModule": true
	}
}
